<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.study.about.IAboutDao">

    <select id="getPianoList" resultType="com.study.about.MyCamelHashMap">
        select
            p_no,
            p_title,
            p_date,
            p_composer,
            p_category
